{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "globals": {
    "process": false
  },
  "exclude":[
    "node_modules",
    "**/__test*", //exclude all that *__test*
    "**/*.spec.ts" // exclude all that contain "spec.ts" in name
  ],
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
